
<%- include('head'); %>



<div class="container-fluid">
<p>Admin</p>
    <table class="table">
        <tr>
           <th>Type</th><th>Date</th><th>Severity</th><th>Description</th><th>Status</th><th>Update</th>
        </tr>
     
        <% for (var i = 0; i < tkts.length; i++) { %>
         <tr>
           <td><%= tkts[i].Type %></td>
           <td><%= tkts[i].Date %></td>
           <td><%= tkts[i].Severity %></td>
           <td><%= tkts[i].Description %></td>
           <td><%= tkts[i].Status %></td>
           <td><a href="/updateTkt?id=<%= tkts[i].id %>">Update</a></td>
         </tr>    
        <% } %>
     </table>
</div>